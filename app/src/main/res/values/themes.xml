<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WoofMateDating" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#654814</item>
        <item name="colorPrimaryVariant">#E2CDB8</item>
        <item name="colorOnPrimary">#E2CDB8</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#D1C1AB</item>
        <item name="colorSecondaryVariant">#654814</item>
        <item name="colorOnSecondary">#E2CDB8</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#654814</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.WoofMateDating.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.WoofMateDating.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.WoofMateDating.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
